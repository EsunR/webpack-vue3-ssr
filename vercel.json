{
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel"
        },
        {
            "src": "*.css",
            "use": "@vercel"
        },
        {
            "src": "*.js",
            "use": "@vercel"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/api/vercel.js"
        }
    ]
}